		int[] pZahlen;
		pZahlen = new int[1000];
		int temp = 1;
		int i = 0;
		int prim = 3;
		pZahlen[0] = 2;
		pZahlen[temp] = 3;
		
		while (temp <= pZahlen.length) {
			while ((i <= temp) && ((prim % pZahlen[i]) == 0)) {
				if (i == temp) {
					pZahlen[temp] = prim;
					i = 0;
					
					System.out.print(pZahlen[temp] + " ");
					temp++;
				}
			}
		}
		
		for (int q = 0; q < pZahlen.length; q++) {
			System.out.print(pZahlen[q] + " ");
		}
		// for (int kapital==1000);
	}
	
}
